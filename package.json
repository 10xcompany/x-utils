{
  "name": "@10xcompany/utils",
  "version": "0.0.2",
  "description": "utilities library for project x",
  "main": "dist/index.js",
  "types": "dist/src/index.d.ts",
  "module": "dist/index.esm.js",
  "jsnext:main": "dist/index.esm.js",
  "repository": "https://github.com/10xcompany/x-utils.git",
  "author": "Quadriphobs1 <abiodunquadriadekunle@gmail.com>",
  "license": "",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "rollup": "^2.33.1",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5"
  },
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "rollup -c rollup.config.js",
    "lint": "prettier --check \"src/**/*.ts\"",
    "lint:fix": "prettier --write \"src/**/*.ts\"",
    "prepare": "yarn run build"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "bugs": {
    "url": "https://github.com/10xcompany/x-utils.git/issues"
  },
  "homepage": "https://github.com/10xcompany/x-utils.git#readme",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {}
}
