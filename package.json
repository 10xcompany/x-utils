{
  "name": "@10xcompany/utils",
  "version": "0.0.2",
  "description": "utilities library for project x",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/10xcompany/x-utils.git",
  "author": "Quadriphobs1 <abiodunquadriadekunle@gmail.com>",
  "license": "NONE",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5"
  },
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "tsc --declaration --project tsconfig.build.json",
    "lint": "prettier --check \"src/**/*.ts\"",
    "lint:fix": "prettier --write \"src/**/*.ts\"",
    "prepare": "yarn run build"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "bugs": {
    "url": "https://github.com/10xcompany/x-utils.git/issues"
  },
  "homepage": "https://github.com/10xcompany/x-utils.git#readme",
  "files": [
    "dist/**/*"
  ]
}
